{
  "name": "x-v2fly",
  "version": "0.0.1",
  "scripts": {
    "deno": "deno run --import-map ./import_map.json -A --unstable",
    "dev": "deno run --import-map ./import_map.json -A --no-check --unstable x-v2fly.ts",
    "dev:web": "deno run --import-map ./import_map.json -A --no-check --unstable web/main.ts",
    "release": "deno run --import-map ./import_map.json -A --unstable scripts/release.ts",
    "test": "deno test --import-map ./import_map.json --allow-env --allow-read --allow-run --unstable"
  }
}